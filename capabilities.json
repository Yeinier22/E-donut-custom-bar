{
  "dataRoles": [
    { "name": "category", "kind": "Grouping", "displayName": "Category" },
    { "name": "legend", "kind": "Grouping", "displayName": "Legend" },
    { "name": "series", "kind": "Measure", "displayName": "Series" }
  ],
  "dataViewMappings": [
    {
      "categorical": {
        "categories": {
          "for": { "in": "category" },
          "dataReductionAlgorithm": { "top": {} }
        },
        "values": {
          "group": {
            "by": "legend",
            "select": [{ "for": { "in": "series" } }],
            "dataReductionAlgorithm": { "top": {} }
          }
        }
      }
    }
  ],
  "supportsHighlight": true,
  "objects": {
    "dataColors": {
      "displayName": "Data Colors",
      "properties": {
        "fill": {
          "displayName": "Color",
          "type": { "fill": { "solid": { "color": true } } }
        }
      }
    },
    "dataPoint": {
      "displayName": "Series Colors",
      "properties": {
        "fill": {
          "displayName": "Color",
          "type": { "fill": { "solid": { "color": true } } }
        }
      }
    },
    "dataLabels": {
      "displayName": "Data Labels",
      "properties": {
        "show": {
          "displayName": "Show Labels",
          "type": { "bool": true }
        }
      }
    },
    "yAxis": {
      "displayName": "Y Axis",
      "properties": {
        "showGridlines": {
          "displayName": "Show Gridlines",
          "type": { "bool": true }
        }
      }
    }
  },
  "privileges": []
}