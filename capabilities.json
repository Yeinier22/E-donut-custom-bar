{
  "dataRoles": [
    { "name": "category", "kind": "Grouping", "displayName": "Category" },
    { "name": "legend", "kind": "Grouping", "displayName": "Legend" },
    { "name": "series", "kind": "GroupingOrMeasure", "displayName": "Series" }
  ],
  "dataViewMappings": [
    {
      "categorical": {
        "categories": {
          "select": [
            { "for": { "in": "category" } }
          ],
          "dataReductionAlgorithm": { "top": {} }
        },
        "values": {
          "group": {
            "by": "legend",
            "select": [
              { "bind": { "to": "series" } }
            ],
            "dataReductionAlgorithm": { "top": {} }
          }
        }
      }
    }
  ],
  "supportsHighlight": true,
  "objects": {
    "dataPoint": {
      "displayName": "Data colors",
      "properties": {
        "fill": {
          "displayName": "Color",
          "type": { "fill": { "solid": { "color": true } } }
        }
      }
    },
    "dataLabels": {
      "displayName": "Data Labels",
      "properties": {
        "show": {
          "displayName": "Show Data Labels",
          "type": { "bool": true }
        },
        "series": {
          "displayName": "Series",
          "type": { "enumeration": [ { "displayName": "All", "value": "all" } ] }
        },
        "position": {
          "displayName": "Position",
          "type": {
            "enumeration": [
              { "displayName": "Auto", "value": "auto" },
              { "displayName": "Inside end", "value": "insideEnd" },
              { "displayName": "Outside end", "value": "outsideEnd" },
              { "displayName": "Inside center", "value": "insideCenter" },
              { "displayName": "Inside base", "value": "insideBase" }
            ]
          }
        },
        "fontFamily": {
          "displayName": "Font Family",
          "type": { "text": true }
        },
        "fontSize": {
          "displayName": "Font Size",
          "type": { "numeric": true }
        },
        "fontStyle": {
          "displayName": "Font Style",
          "type": {
            "enumeration": [
              { "displayName": "Normal", "value": "normal" },
              { "displayName": "Bold", "value": "bold" },
              { "displayName": "Italic", "value": "italic" }
            ]
          }
        },
        "color": {
          "displayName": "Color",
          "type": { "fill": { "solid": { "color": true } } }
        },
        "transparency": {
          "displayName": "Transparency",
          "type": { "numeric": true }
        }
      }
    },
    "xAxis": {
      "displayName": "X Axis",
      "properties": {
        "showAxisLine": {
          "displayName": "Show Axis Line",
          "type": { "bool": true }
        },
        "showLabels": {
          "displayName": "Show Labels",
          "type": { "bool": true }
        },
        "showGridLines": {
          "displayName": "Show Grid Lines",
          "type": { "bool": true }
        },
        "labelColor": {
          "displayName": "Label Color",
          "type": { "fill": { "solid": { "color": true } } }
        },
        "labelSize": {
          "displayName": "Label Font Size",
          "type": { "formatting": { "fontSize": true } }
        },
        "rotateLabels": {
          "displayName": "Rotate Labels (degrees)",
          "type": { "numeric": true }
        },
        "fontFamily": {
          "displayName": "Font Family",
          "type": { "formatting": { "fontFamily": true } }
        },
        "fontStyle": {
          "displayName": "Font Style",
          "type": {
            "enumeration": [
              { "displayName": "Regular", "value": "regular" },
              { "displayName": "Bold", "value": "bold" },
              { "displayName": "Italic", "value": "italic" },
              { "displayName": "Bold and italic", "value": "boldItalic" }
            ]
          }
        }
      }
    },
    "yAxis": {
      "displayName": "Y Axis",
      "properties": {
        "showLabels": {
          "displayName": "Show Labels",
          "type": { "bool": true }
        },
        "showGridLines": {
          "displayName": "Show Grid Lines",
          "type": { "bool": true }
        },
        "labelColor": {
          "displayName": "Label Color",
          "type": { "fill": { "solid": { "color": true } } }
        },
        "labelSize": {
          "displayName": "Label Font Size",
          "type": { "formatting": { "fontSize": true } }
        },
        "fontFamily": {
          "displayName": "Font Family",
          "type": { "formatting": { "fontFamily": true } }
        },
        "fontStyle": {
          "displayName": "Font Style",
          "type": {
            "enumeration": [
              { "displayName": "Regular", "value": "regular" },
              { "displayName": "Bold", "value": "bold" },
              { "displayName": "Italic", "value": "italic" },
              { "displayName": "Bold and italic", "value": "boldItalic" }
            ]
          }
        }
      }
    },
    "legend": {
      "displayName": "Legend",
      "properties": {
        "position": {
          "displayName": "Position",
          "type": {
            "enumeration": [
              { "displayName": "Top", "value": "top" },
              { "displayName": "Bottom", "value": "bottom" },
              { "displayName": "Left", "value": "left" },
              { "displayName": "Right", "value": "right" }
            ]
          }
        },
        "alignment": {
          "displayName": "Alignment",
          "type": {
            "enumeration": [
              { "displayName": "Left", "value": "left" },
              { "displayName": "Center", "value": "center" },
              { "displayName": "Right", "value": "right" }
            ]
          }
        },
        "iconShape": {
          "displayName": "Icon Shape",
          "type": {
            "enumeration": [
              { "displayName": "Rectangle", "value": "rect" },
              { "displayName": "Square", "value": "square" },
              { "displayName": "Circle", "value": "circle" }
            ]
          }
        },
        "padding": {
          "displayName": "Padding",
          "description": "Legend padding in pixels",
          "type": { "numeric": true }
        }
        ,
        "extraMargin": {
          "displayName": "Extra Margin (%)",
          "type": { "numeric": true }
        }
      }
    }
  },
  "privileges": []
}